<UserControl x:Class="EditorWPF.ResourceView.ResourceView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:EditorWPF.ResourceView"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <WrapPanel Background="Red" x:Name="resource_list">
        <WrapPanel.ContextMenu>
            <ContextMenu>
                <MenuItem Header="Import mesh" Click="ImportMesh"></MenuItem>
            </ContextMenu>
        </WrapPanel.ContextMenu>
    </WrapPanel>

</UserControl>
